{
  "name": "gbt-ubs-translator",
  "displayName": "UBS Translator",
  "description": "An editor for translating UBS dictionary files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "gbt.ubsTranslator",
        "displayName": "GBT UBS Translator",
        "selector": [
          {
            "filenamePattern": "*.xml"
          }
        ]
      },
      {
        "viewType": "gbt.ubsViewer",
        "displayName": "GBT UBS Viewer",
        "selector": [
          {
            "filenamePattern": "*.xml"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  }
}
